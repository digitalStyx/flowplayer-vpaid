{
  "name": "flowplayer-vpaid",
  "version": "1.0.0",
  "dependencies": {
    "vpaid-html5-client": "0.1.11"
  },
  "devDependencies": {
    "browserify": "12.0.1",
    "uglifyify": "3.0.1",
    "brfs": "1.4.2"
  },
  "scripts": {
    "build": "browserify src/vpaid.js -o src/temp.js && brfs src/temp.js > dist/vpaid.js && rm src/temp.js && browserify -g uglifyify dist/vpaid.js -o dist/vpaid.min.js"
  },
  "description": "VPAID 2.0 (JS) support within Flowplayer 6 HTML",
  "main": "src/vpaid.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mantisadnetwork/flowplayer-vpaid.git"
  },
  "keywords": [
    "video",
    "vpaid",
    "flowplayer"
  ],
  "author": "Paris Holley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mantisadnetwork/flowplayer-vpaid/issues"
  },
  "homepage": "https://github.com/mantisadnetwork/flowplayer-vpaid#readme"
}
